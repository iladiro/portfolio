<section class="cv-section">
  <div class="cv-section__head">
    <div class="cv-section__icon">
      <i class="fa fa-code"></i>
    </div>
    <h2 class="cv-section__label text-uppercase">Personal Projects</h2>
  </div>

  <% data.curriculum.prj.personal_projects.each do |project| %>
    <%= partial("partials/shared/cv-block",
        locals: {
          label: project.label,
          title: project.title,
          description: project.description,
          github: project.github
        }
      )
    %>
  <% end %>

</section>
