<section class="cv-section">
  <div class="cv-section__head">
    <div class="cv-section__icon">
      <i class="fa fa-pencil"></i>
    </div>
    <h2 class="cv-section__label text-uppercase">Personal skills and competences</h2>
  </div>
  
  <% data.curriculum.prj.skills.each do |skill| %>
    <%= partial("partials/shared/cv-block",
        locals: {
          label: skill.label,
          title: skill.title,
          description: skill.description
        }
      )
    %>
  <% end %>

</section>
