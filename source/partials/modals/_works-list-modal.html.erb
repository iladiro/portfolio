
<% data.works.list.each do |item| %>
  <%= partial('partials/modals/work-modal',
    locals: {
      id: item.id,
      tech_list: item.technologies,
      title: item.title,
      image: image_path(item.image),
      description: item.description,
      link: item.link,
      alt_text: item.alt_text
    })
  %>
<% end %>